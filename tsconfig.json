{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "skipLibCheck": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@app/upload": [
        "libs/upload/src"
      ],
      "@app/upload/*": [
        "libs/upload/src/*"
      ]
    }
  }
}