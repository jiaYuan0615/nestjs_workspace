{
  "name": "nestjs_workspace",
  "version": "0.0.1",
  "description": "nestjs with domain-driven-design base workspace",
  "author": "JiaYuan",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:inventory": "nest build --webpack --webpackPath ./apps/inventory/webpack-hmr.config.js --watch",
    "build:ecommerce": "nest build --webpack --webpackPath ./apps/ecommerce/webpack-hmr.config.js --watch",
    "webpack:inventory": "webpack --config ./apps/inventory/webpack.config.js",
    "webpack:ecommerce": "webpack --config ./apps/ecommerce/webpack.config.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nodemon --exec ts-node src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs_workspace/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.7",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.2.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.2",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "jyz-common-library-type": "^1.0.10",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "mssql": "^9.1.1",
    "mysql2": "^2.3.3",
    "nocache": "^3.0.1",
    "oracledb": "^5.5.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.5",
    "typeorm-extension": "^2.1.1",
    "uuid": "^8.3.2",
    "webp-converter": "^2.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^8.0.29",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.16",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.4",
    "webpack": "^5.71.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {}
  }
}
